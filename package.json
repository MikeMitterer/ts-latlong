{
    "name": "@mmit/latlong",
    "author": "Mike Mitterer",
    "version": "0.1.6",
    "description": "Lightweight library for common latitude and longitude calculation",
    "keywords": [
        "typescript lattitude longitude latlong"
    ],
    "license": "MIT",
    "type-check": "tsc",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "gen:barrels": "barrelsby --singleQuotes --delete -d src/main --exclude ConfigLog4j --exclude 'events/utils' -l top -s flat",
        "serve": "NODE_ENV=development webpack-dev-server --config webpack.web.js --mode development",
        "start": "yarn serve --open",
        "tc": "tsc --incremental false --noEmit",
        "tc:watch": "npm run tc -- --watch",
        "lint": "tslint --project tsconfig.json",
        "test": "yarn run gen:barrels && yarn run test:unit && yarn run test:e2e",
        "test:unit": "jest --colors src/test/unit",
        "test:integration": "jest --colors --runInBand --detectOpenHandles src/test/integration",
        "test:e2e": "E2E_TEST='true' jest --colors --runInBand --detectOpenHandles src/test/e2e",
        "build": "rm -rf lib && yarn run gen:barrels && yarn run build:ts",
        "build:ts": "tsc --rootDir $(pwd)/src/main --project tsconfig.lib.json --declaration --outDir $(pwd)/lib",
        "build:ts:watch": "yarn build:ts --watch",
        "build:types": "rm -rf lib && yarn build:ts --emitDeclarationOnly",
        "build:js": "rm -rf lib && NODE_ENV=development webpack --config webpack.ts.js --mode development",
        "build:js:prod": "rm -rf lib && NODE_ENV=production webpack --config webpack.ts.js --mode production",
        "build:babel": "rm -rf lib && REQUIRE_TARGET='browser' babel src/main --out-dir lib --extensions \".ts,.tsx\" --source-maps",
        "build:node": "rm -rf bin && NODE_ENV=development webpack --config webpack.node.js --mode development",
        "build:node:prod": "NODE_ENV=production webpack --config webpack.node.js --mode production",
        "build:web": "rm -rf dist && NODE_ENV=development webpack --config webpack.web.js --mode development",
        "build:web:prod": "rm -rf dist && NODE_ENV=production webpack --config webpack.web.js --mode production",
        "deploy": "yarn version --new-version patch && yarn publish --access public --non-interactive",
        "doc": "typedoc --out doc --tsconfig tsconfig.lib.json",
        "clean": "rm -rf lib dist bin doc && jest --clearCache",
        "prettier": "onchange './src/**/*.ts'  -- prettier --write {{changed}}"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/ts-latlong"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "@mmit/validate": "^0.3.2",
        "global": "^4.4.0",
        "typescript-collections": "1.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@types/core-js": "^2.5.3",
        "@types/expect-puppeteer": "4.4.0",
        "@types/jest": "^25.1.4",
        "@types/node": "^13.1.8",
        "@types/puppeteer": "^2.0.1",
        "@types/qunit": "^2.9.0",
        "@types/shelljs": "^0.8.7",
        "@types/ws": "^7.2.3",
        "autoprefixer": "^9.7.5",
        "awesome-typescript-loader": "^5.2.1",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "babel-preset-modern-browsers": "^14.1.1",
        "barrelsby": "^2.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.2",
        "file-loader": "^6.0.0",
        "html-beautify-webpack-plugin": "^1.0.5",
        "html-loader": "^1.0.0",
        "html-webpack-plugin": "^3.2.0",
        "html-webpack-string-replace-plugin": "^1.1.0",
        "jest": "^25.1.0",
        "jest-cli": "^25.1.0",
        "jest-extended": "^0.11.5",
        "jest-haste-map": "^25.1.0",
        "jest-puppeteer": "^4.4.0",
        "jest-resolve": "^25.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "moment": "^2.24.0",
        "node-sass": "^4.13.1",
        "postcss": "^7.0.27",
        "postcss-loader": "^3.0.0",
        "puppeteer": "^2.1.1",
        "qunit": "^2.9.3",
        "sass-loader": "^8.0.2",
        "semver": "^7.1.3",
        "shelljs": "^0.8.3",
        "source-map-loader": "^0.2.4",
        "style-loader": "^1.1.3",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.2",
        "ts-node": "^8.8.1",
        "tslint": "^6.1.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-loader": "^3.5.4",
        "tslint-override": "^0.6.0",
        "typedoc": "^0.17.3",
        "typescript": "^3.8.3",
        "url-loader": "^4.0.0",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-livereload-plugin": "^2.3.0",
        "webpack-node-externals": "^1.7.2"
    }
}
