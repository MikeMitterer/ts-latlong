{
    "name": "@mmit/latlong",
    "author": "Mike Mitterer",
    "version": "0.2.10",
    "description": "Lightweight library for common latitude and longitude calculation",
    "type": "module",
    "keywords": [
        "typescript lattitude longitude latlong"
    ],
    "license": "MIT",
    "type-check": "tsc",
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "version:pkg": "jq -r '.version' < package.json",
        "version:pkg:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:pkg)",
        "version:remote": "curl --silent -X GET -H \"Authorization: token ${GH_TOKEN_READ_PRIVATE_REPO}\" \"https://raw.githubusercontent.com/$(cat ./.gitorg)/$(git repo)/master/package.json\" | jq -r \".version\" ",
        "version:remote:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:remote)",
        "update": "yarn update:browser",
        "update:browser": "browserslist --update-db",
        "update:templates": "updateDevTemplates.sh -u",
        "clean": "rm -rf lib dist bin",
        "clean:jest": "jest --clearCache",
        "clean:all": "yarn clean && yarn clean:jest && rm -f yarn.lock && rm -rf node_modules && yarn clean cache",
        "gen:barrels": "barrelsby --singleQuotes --delete -d src/main --exclude ConfigLog4j --exclude 'events/utils' -l top -s flat",
        "serve": "NODE_ENV=development webpack serve --config webpack.web.cjs --mode development",
        "start": "yarn serve --open",
        "tc": "tsc --incremental false --noEmit",
        "tc:watch": "npm run tc -- --watch",
        "lint": "yarn eslint src",
        "lint:fix": "yarn eslint src --fix",
        "test": "yarn run gen:barrels && yarn run test:unit && yarn run test:e2e",
        "test:unit": "jest --colors tests/unit",
        "test:e2e": "E2E_TEST='true' jest --colors --runInBand --detectOpenHandles tests/e2e",
        "build": "yarn run clean && yarn run gen:barrels && yarn run build:rup",
        "build:rup": "rollup -c",
        "build:ts": "tsc --rootDir $(pwd)/src/main --project tsconfig.lib.json --declaration --outDir $(pwd)/lib",
        "build:ts:watch": "yarn build:ts --watch",
        "build:types": "rm -rf lib && yarn build:ts --emitDeclarationOnly",
        "build:js": "rm -rf lib && NODE_ENV=development webpack --config webpack.ts.cjs --mode development",
        "build:js:prod": "rm -rf lib && NODE_ENV=production webpack --config webpack.ts.cjs --mode production",
        "build:babel": "rm -rf lib && REQUIRE_TARGET='browser' babel src/main --out-dir lib --extensions \".ts,.tsx\" --source-maps",
        "build:node": "rm -rf bin && NODE_ENV=development webpack --config webpack.node.cjs --mode development",
        "build:node:prod": "NODE_ENV=production webpack --config webpack.node.cjs --mode production",
        "build:web": "rm -rf dist && NODE_ENV=development webpack --config webpack.web.cjs --mode development",
        "build:web:prod": "rm -rf dist && NODE_ENV=production webpack --config webpack.web.cjs --mode production",
        "deploy": "yarn version --new-version patch && yarn publish --access public --non-interactive",
        "doc": "typedoc --out doc --tsconfig tsconfig.lib.json",
        "prettier": "onchange './src/**/*.ts'  -- prettier --write {{changed}}"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/ts-latlong"
    },
    "dependencies": {
        "@mmit/logging": "0.1.13",
        "@mmit/validate": "0.3.14",
        "global": "^4.4.0",
        "typescript-collections": "1.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.10",
        "@babel/core": "^7.22.11",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-typescript": "^7.22.11",
        "@babel/runtime": "^7.22.11",
        "@mmit/muni": "0.0.5",
        "@mmit/styles": "^0.1.33",
        "@rollup/plugin-node-resolve": "15.2.1",
        "@rollup/plugin-replace": "5.0.2",
        "@rollup/plugin-typescript": "11.1.3",
        "@types/chai": "^4.3.5",
        "@types/core-js": "^2.5.4",
        "@types/eslint": "^8",
        "@types/jest": "29.5.4",
        "@types/node": "^20.5.7",
        "@types/shelljs": "^0.8.9",
        "@types/ws": "^7.4.7",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "autoprefixer": "^10.4.15",
        "babel-jest": "29.7.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-inline-environment-variables": "^0.4.3",
        "barrelsby": "^2.8.0",
        "beautify-html-webpack-plugin": "^1.0.3",
        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "chai": "^4.3.7",
        "clean-terminal-webpack-plugin": "^3.0.0",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "copy-webpack-plugin": "^9.0.1",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.2.0",
        "date-fns": "2.23.0",
        "ejs-compiled-loader": "^3.1.0",
        "eslint": "^8.48.0",
        "file-loader": "^6.2.0",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
        "html-webpack-string-replace-plugin": "^1.1.0",
        "https-browserify": "^1.0.0",
        "indexes-of": "^1.0.1",
        "jest": "29.7.0",
        "jest-cli": "29.7.0",
        "jest-extended": "2.0.0",
        "jest-haste-map": "29.7.0",
        "jest-puppeteer": "10.1.0",
        "jest-resolve": "29.7.0",
        "json5": "^2.2.0",
        "mini-css-extract-plugin": "^2.2.0",
        "moment": "^2.29.1",
        "path-browserify": "^1.0.1",
        "postcss": "^8.4.29",
        "postcss-loader": "^7.3.3",
        "process": "^0.11.10",
        "puppeteer": "23.2.0",
        "rollup": "3.29.2",
        "sass": "~1.32",
        "sass-loader": "10",
        "semver": "^7.3.5",
        "shelljs": "^0.8.4",
        "source-map-loader": "3.0.0",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "style-loader": "^3.2.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "typedoc": "^0.25",
        "typedoc-plugin-markdown": "^3.10.2",
        "typescript": "^5.2.2",
        "uniq": "^1.0.1",
        "webpack": "5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-livereload-plugin": "3.0.2",
        "webpack-node-externals": "3.0.0"
    }
}
